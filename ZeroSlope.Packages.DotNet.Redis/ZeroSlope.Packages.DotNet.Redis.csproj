<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>ZeroSlope.Packages.DotNet.Redis</PackageId>
    <Version>1.0.0</Version>
    <Authors>cfryerdev@gmail.com</Authors>
    <Company>ZeroSlope</Company>
    <Description> Redis connectivity support for the dotnet ZeroSlope architecture. </Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
    <PackageReference Include="StackExchange.Redis" Version="2.2.88" />
    <PackageReference Include="ZeroSlope.Packages.DotNet.Exceptions" Version="1.0.0" />
  </ItemGroup>

</Project>
